@using BabyCareProject.Dtos.BannerDtos
@model List<ResultBannerDto>
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@

@foreach(var item in Model)
{
<div class="container-fluid py-5 hero-header wow fadeIn" data-wow-delay="0.1s">
    <div class="container py-5">
        <div class="row g-5">
            <div class="col-lg-7 col-md-12">
                <h1 class="mb-3 text-primary">@item.Title</h1>
                <h1 class="mb-5 display-1 text-white">@item.Description</h1>
                <a href="" class="btn btn-primary px-4 py-3 px-md-5  me-4 btn-border-radius">Başlayalım</a>
                <a href="" class="btn btn-primary px-4 py-3 px-md-5 btn-border-radius">Daha Fazlası</a>
            </div>
        </div>
    </div>
</div>
}